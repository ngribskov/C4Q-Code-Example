<body>
  <% @day.each do |j| %>
  <div class="day" id = <%=j[6]%>>
    <%= j[0].to_date %><br/>
    <%= image_tag "#{j[1]}" %><br/>
    <div class = "lo" >
      lo <%= j[2].to_s %>
    </div>
    <div class = "hi" >
      hi <%= j[4].to_s %>
    </div>
  </div>
  <% end %>
  <br/>
  <%= react_component('TemperatureToggle',
    minTempC: @minC,
    minTempF: @minF,
    maxTempC: @maxC,
    maxTempF: @maxF) %>

</body>
